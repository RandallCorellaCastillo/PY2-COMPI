non terminal String PRINTLN, RETURN_STATEMENT, FUNC_STAT_DEF, BREAK_STATEMENT, INPUT_STATEMENT;

/* Precedences */
precedence left PLUS, MINUS;
precedence left TIMES;
precedence left DIV;

/* The grammar rules */
start with PROGRAMA;
//Inicial.
