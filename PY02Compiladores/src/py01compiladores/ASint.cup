                BODY
                {: TablaSimbolosToString(); toStringCod3D();:};

//cuerpo del programa.
BODY ::= BODY STATEMENT | STATEMENT;

//Lineas del codigo.
STATEMENT::= 
     FUNC_STAT_DEF
     | DECL
     | error ENDLINE
     | error UNDERS;

//Lineas recursivas.
LINE::=
      LINER | LINE LINER;
//Cuerpo de los statements.
LINER::=
        ASIG
      | DECLOC
      | DECL
      | FOR_STATEMENT
      | WHILE_STATEMENT
      | SWITCH_STATEMENT
      | IF_STATEMENT
      | EXP
      | PARAMDECL
      | PRINTLN
      | RETURN_STATEMENT
      | BREAK_STATEMENT
      | INPUT_STATEMENT;

